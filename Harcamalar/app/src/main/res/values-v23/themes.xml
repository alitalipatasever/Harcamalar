<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="Theme.Harcamalar" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/bg</item>
        <item name="colorPrimaryVariant">@color/bg</item>
        <item name="colorOnPrimary">@color/bg</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/bg</item>
        <item name="colorSecondaryVariant">@color/bg</item>
        <item name="colorOnSecondary">@color/bg</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <item name="android:windowLightStatusBar">true</item>
        <!-- Customize your theme here. -->

        <item name="fontFamily">@font/poppins</item>
        <item name="android:textColor">@color/lacivert</item>

